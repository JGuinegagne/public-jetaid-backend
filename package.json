  "dependencies": {
    "@types/express": "^4.11.1",
    "@types/geolib": "^2.0.23",
    "@types/node-geocoder": "^3.19.0",
    "@types/passport": "^0.4.5",
    "@types/passport-local": "^1.0.33",
    "@types/sequelize": "^4.28.7",
    "airport-codes": "^1.0.2",
    "all-the-cities": "^2.0.1",
    "cors": "^2.8.4",
    "countries-list": "^2.3.2",
    "csvjson": "^5.0.0",
    "currency-codes": "^1.3.0",
    "dotenv": "^5.0.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "geolib": "^2.0.24",
    "gulp": "^4.0.2",
    "jsonfile": "^4.0.0",
