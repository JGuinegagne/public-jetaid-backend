{
  "name": "jetaid-backend",
  "version": "1.0.0",
  "description": "Jetaid server definition",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "restart": "pm2 startOrRestart ecosystem.config.js",
    "deploy": "pm2 deploy ecosystem.config.js production",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "rollback": "node_modules/.bin/sequelize db:migrate:undo",
    "seed": "node_modules/.bin/sequelize db:seed --seed '20180906154350-rides.js'",
    "unseed": "node_modules/.bin/sequelize db:seed:undo --seed '20180906154350-rides.js'",
    "seedall": "node_modules/.bin/sequelize db:seed:all",
    "unseedall": "node_modules/.bin/sequelize db:seed:undo:all",
    "unseedtrips": "node_modules/.bin/sequelize db:seed:undo --seed '20180620210353-trips.js'",
    "unseedprofiles": "node_modules/.bin/sequelize db:seed:undo --seed '20180524124539-profileInfos.js'",
    "unseedtravelers": "node_modules/.bin/sequelize db:seed:undo --seed '20180521123607-demo-travelers'",
    "unseedusers": "node_modules/.bin/sequelize db:seed:undo --seed '20180520143017-demo-users.js'",
    "unseedflights": "node_modules/.bin/sequelize db:seed:undo --seed '20180519165058-flights.js'",
    "unseedgeographics": "node_modules/.bin/sequelize db:seed:undo --seed '20180510124352-geographics.js'",
    "seedtrips": "node_modules/.bin/sequelize db:seed --seed '20180620210353-trips.js'",
    "seedprofiles": "node_modules/.bin/sequelize db:seed --seed '20180524124539-profileInfos.js'",
    "seedtravelers": "node_modules/.bin/sequelize db:seed --seed '20180521123607-demo-travelers'",
    "seedusers": "node_modules/.bin/sequelize db:seed --seed '20180520143017-demo-users.js'",
    "seedflights": "node_modules/.bin/sequelize db:seed --seed '20180519165058-flights.js'",
    "seedgeographics": "node_modules/.bin/sequelize db:seed --seed '20180510124352-geographics.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k-o-r-t-o/jetaid-backend-js.git"
  },
  "author": "Jonathan Guinegagne",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/k-o-r-t-o/jetaid-backend-js/issues"
